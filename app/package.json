{
	"name": "microservice-starter",
	"version": "0.0.1",
	"scripts": {
		"db:migrate": "scripts/migration.sh",
		"build": "rm -rf ./dist && tsc --build tsconfig.prod.json",
		"start": "node -r module-alias/register ./dist",
		"start:dev": "nodemon --config nodemon.json",
		"start:staging": "node -r module-alias/register ./dist --env staging",
		"test": "nodemon --config nodemon.test.json",
		"lint": "eslint '*/**/*.{js,ts,tsx}' .gitignore .",
		"lintfix": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix --ignore-path .gitignore ."
	},
	"_moduleAliases": {
		"@db": "dist/db",
		"@daos": "dist/daos",
		"@controllers": "dist/controllers",
		"@middlewares": "dist/middlewares",
		"@models": "dist/models",
		"@routes": "dist/routes",
		"@shared": "dist/shared",
		"@config": "dist/config",
		"@server": "dist/server",
		"@types": "dist/types"
	},
	"dependencies": {
		"bcrypt": "^5.0.0",
		"class-transformer": "^0.4.0",
		"class-validator": "^0.13.1",
		"command-line-args": "^5.1.1",
		"cookie-parser": "^1.4.5",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"express-async-errors": "^3.1.1",
		"express-validator": "^6.5.0",
		"helmet": "^3.22.0",
		"http-errors": "^1.7.3",
		"http-status-codes": "^1.4.0",
		"ip-regex": "^4.1.0",
		"jsonwebtoken": "^8.5.1",
		"module-alias": "^2.2.2",
		"moment": "^2.28.0",
		"morgan": "^1.10.0",
		"mysql2": "^2.3.2",
		"nodemailer": "^6.4.14",
		"reflect-metadata": "^0.1.13",
		"routing-controllers": "^0.9.0",
		"twilio": "^3.50.0",
		"typeorm": "^0.2.38",
		"validator": "^13.1.17",
		"winston": "^3.2.1"
	},
	"devDependencies": {
		"@types/bcrypt": "^3.0.0",
		"@types/command-line-args": "^5.0.0",
		"@types/cookie-parser": "^1.4.2",
		"@types/cors": "^2.8.6",
		"@types/express": "^4.17.8",
		"@types/find": "^0.2.1",
		"@types/graphql-upload": "^8.0.4",
		"@types/helmet": "0.0.47",
		"@types/http-errors": "^1.6.3",
		"@types/jasmine": "^3.5.14",
		"@types/jsonfile": "^6.0.0",
		"@types/jsonwebtoken": "^8.5.0",
		"@types/morgan": "^1.9.0",
		"@types/multer": "^1.4.4",
		"@types/node": "^14.10.1",
		"@types/supertest": "^2.0.9",
		"@types/validator": "^13.1.0",
		"@typescript-eslint/eslint-plugin": "^3.10.1",
		"@typescript-eslint/parser": "^3.10.1",
		"eslint": "^7.9.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.3",
		"find": "^0.3.0",
		"fs-extra": "^9.0.0",
		"jasmine": "^3.5.0",
		"jsonfile": "^6.0.1",
		"nodemon": "^2.0.4",
		"prettier": "^2.1.1",
		"supertest": "^4.0.2",
		"ts-node": "^8.10.1",
		"tsconfig-paths": "^3.9.0",
		"tslint": "^6.1.2",
		"typescript": "^3.9.3"
	}
}
